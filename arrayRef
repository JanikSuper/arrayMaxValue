class Program
    {
        static void Main(string[] args)
        {
            int[] array = { 1, 5, 7, 9, 8 };
            int maxNumber = int.MinValue; 

            for (int i = 0; i < array.Length; i++)
            {
                if (maxNumber < array[i])
                {
                    maxNumber = array[i];
                }
            }

            Console.WriteLine(maxNumber);
        }            
    }
